{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "9.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "2.0.0" }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "iteration": 1,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "stat",
      "title": "Goroutines",
      "id": 1,
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "runtime_goroutines{job=~\"$job\", instance=~\"$instance\", app=~\"$app\"}",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "value"
      },
      "fieldConfig": {
        "defaults": { "decimals": 0, "unit": "none" },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Heap Alloc (bytes)",
      "id": 2,
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "runtime_mem_heap_alloc_bytes{job=~\"$job\", instance=~\"$instance\", app=~\"$app\"}",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "type": "timeseries",
      "title": "Heap Sys (bytes)",
      "id": 3,
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "runtime_mem_sys_bytes{job=~\"$job\", instance=~\"$instance\", app=~\"$app\"}",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "type": "timeseries",
      "title": "GC Rate (count/s)",
      "id": 4,
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "rate(runtime_gc_count_total{job=~\"$job\", instance=~\"$instance\", app=~\"$app\"}[5m])",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "type": "timeseries",
      "title": "Avg GC Pause per GC (s)",
      "id": 5,
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "( rate(runtime_gc_pause_total_ms{job=~\"$job\", instance=~\"$instance\", app=~\"$app\"}[5m]) / 1000 ) / rate(runtime_gc_count_total{job=~\"$job\", instance=~\"$instance\", app=~\"$app\"}[5m])",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 37,
  "style": "dark",
  "tags": ["prometheus", "go", "runtime", "quant_system"],
  "templating": {
    "list": [
      {
        "name": "job",
        "type": "query",
        "label": "job",
        "hide": 0,
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(runtime_goroutines, job)",
        "refresh": 2,
        "includeAll": true,
        "allValue": ".*",
        "current": { "text": "All", "value": "$__all" }
      },
      {
        "name": "instance",
        "type": "query",
        "label": "instance",
        "hide": 0,
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(runtime_goroutines{job=~\"$job\"}, instance)",
        "refresh": 2,
        "includeAll": true,
        "allValue": ".*",
        "current": { "text": "All", "value": "$__all" }
      },
      {
        "name": "app",
        "type": "query",
        "label": "app",
        "hide": 0,
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(runtime_goroutines{job=~\"$job\"}, app)",
        "refresh": 2,
        "includeAll": true,
        "allValue": ".*",
        "current": { "text": "All", "value": "$__all" }
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": { "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"] },
  "timezone": "",
  "title": "Go Runtime / quant_system",
  "uid": null,
  "version": 1,
  "weekStart": ""
}
